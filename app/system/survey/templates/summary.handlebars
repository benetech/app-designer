<div>
    <div>
        <center><h2>Survey Summary</h2></center>
        <div>
            {{#each summaryQuestion}}
                {{#ifEqual this.type "location"}}
                    <b>location:</b><br />
                    latitude: {{this.value.latitude}}<br />
                    longitude: {{this.value.longitude}}<br />
                    altitude: {{this.value.altitude}}<br />
                    accuracy: {{this.value.accuracy}}<br />
                {{/ifEqual}}
                {{#ifNotEqual this.type "location"}}
                    <b>{{this.name}}</b>: {{this.value}}<br />
                {{/ifNotEqual}}
            {{else}}
                No content
            {{/each}}
        </div>
    </div>
    <hr/>
    {{#if indicators}}
        <div>
            <h3>Dimension A</h3>
            {{#each indicators}}
                {{#if this.value}}
                    <span class="glyphicon glyphicon-one-fine-dot"
                        style="color:
                        {{this.value}}">
                    </span>
                    <p class="summary-indicator">{{this.name}}: {{this.data_value}}</p>
                {{/if}}
            {{/each}}
        </div>
    {{/if}}
</div>
